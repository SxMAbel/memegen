    "types": "rtb --dist dist"
  },
  "devDependencies": {
    "@favware/rollup-type-bundler": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
